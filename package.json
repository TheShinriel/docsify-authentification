{
  "name": "docsify-authentication",
  "version": "0.0.1",
  "description": "add authentication for some calls of docsify",
  "main": "src",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack src/index.js -o dist/docsify-authentication.js --mode=development",
    "build:prod": "webpack src/index.js -o dist/docsify-authentication.min.js --mode=production",
    "build:example": "webpack src/index.js -o example/docsify-authentication.js --mode=development"
  },
  "keywords": [
    "docsify",
    "authentication",
    "auth"
  ],
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "author": "Shinriel (shinriel.dev@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
